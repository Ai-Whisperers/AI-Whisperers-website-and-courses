{
  "name": "@aiwhisperers/state-core",
  "version": "0.1.0",
  "private": true,
  "description": "State management package for AI Whisperers platform (Zustand + React Query)",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./courses": {
      "types": "./courses/src/index.ts",
      "default": "./courses/src/index.ts"
    },
    "./ui": {
      "types": "./ui/src/index.ts",
      "default": "./ui/src/index.ts"
    },
    "./analytics": {
      "types": "./analytics/src/index.ts",
      "default": "./analytics/src/index.ts"
    },
    "./query": {
      "types": "./query/src/index.ts",
      "default": "./query/src/index.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules .turbo"
  },
  "dependencies": {
    "zustand": "^5.0.2",
    "@tanstack/react-query": "^5.64.2",
    "react": "19.1.0"
